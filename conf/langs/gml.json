{
	"types": [
		{
			"type": "string",
			"format": "[name]# = \"[value]\"#;"
		},
		{
			"type:": "real",
			"format": "[name]# = [value]#;"
		},
		{
			"type": "array",
			"format": "[name] = array_create([value]);"
		},
        {
            "type": "boolean",
            "format": "[name]# = [value]#;"
        },
        {
            "type": "enum",
            "format": "enum [name] {#\n[value]\n#}"
        }
	],

	"templates": [
		{
			"name": "function",
			"format": "function [name]([args]) {\n#\treturn [return]#\n}"
		},
		{
			"name": "class",
			"format": "[name] {\n}"
		}
	],

	"singleCommentRule": "//[value]",
	"multiCommentRule": "/*\n[value]\n*/"

}	